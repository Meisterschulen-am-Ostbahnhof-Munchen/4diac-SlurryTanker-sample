<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="E_T_FF_Dual" Comment="Toggle flip flop">
	<Identification Standard="61499-1 Annex A" Description="Copyright (c) 2017 fortiss GmbH &#10;  &#10;This program and the accompanying materials are made &#10;available under the terms of the Eclipse Public License 2.0 &#10;which is available at https://www.eclipse.org/legal/epl-2.0/ &#10; &#10;SPDX-License-Identifier: EPL-2.0" >
	</Identification>
	<VersionInfo Organization="fortiss GmbH" Version="1.0" Author="Alois Zoitl" Date="2017-09-22" Remarks="initial API and implementation and/or initial documentation">
	</VersionInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="CLK" Type="Event" Comment="Clock for triggering a output toggle">
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="EO0" Type="Event" Comment="inform of toggled output 0">
				<With Var="DO0"/>
			</Event>
			<Event Name="EO1" Type="Event" Comment="inform of toggled output 1">
				<With Var="DO1"/>
			</Event>
		</EventOutputs>
		<OutputVars>
			<VarDeclaration Name="DO0" Type="BOOL" Comment="value of flip flop 0"/>
			<VarDeclaration Name="DO1" Type="BOOL" Comment="value of flip flop 1"/>
		</OutputVars>
	</InterfaceList>
	<BasicFB>
		<ECC>
			<ECState Name="START" Comment="Initial State" x="400.0" y="700.0">
			</ECState>
			<ECState Name="S1" Comment="Initialization" x="1231.25" y="662.5">
				<ECAction Algorithm="K1" Output="EO0"/>
			</ECState>
			<ECState Name="S2" Comment="" x="1381.25" y="1075.0">
				<ECAction Algorithm="OFF" Output="EO0"/>
			</ECState>
			<ECState Name="S3" Comment="" x="1550.0" y="1456.25">
				<ECAction Algorithm="K2" Output="EO1"/>
			</ECState>
			<ECState Name="S4" Comment="" x="1712.5" y="1993.75">
				<ECAction Algorithm="OFF" Output="EO1"/>
			</ECState>
			<ECTransition Source="START" Destination="S1" Condition="CLK" Comment="" x="856.25" y="587.5"/>
			<ECTransition Source="S1" Destination="S2" Condition="CLK" Comment="" x="1375.0" y="937.5"/>
			<ECTransition Source="S2" Destination="S3" Condition="CLK" Comment="" x="1562.5" y="1312.5"/>
			<ECTransition Source="S3" Destination="S4" Condition="CLK" Comment="" x="1731.25" y="1825.0"/>
			<ECTransition Source="S4" Destination="S1" Condition="CLK" Comment="" x="1000.0" y="1500.0"/>
		</ECC>
		<Algorithm Name="TOGGLE" Comment="">
			<ST><![CDATA[ALGORITHM TOGGLE
Q := NOT Q;
END_ALGORITHM

]]></ST>
		</Algorithm>
	</BasicFB>
</FBType>
