<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="Q_NumericValue" Comment="Command change numeric value ( Part 6 - F.22 )">
	<Identification Standard="ISO 11783-6" Description="&lt;p&gt;This command is used to change the value of an object. It applies only to objects that have a numeric value attribute.&amp;nbsp;&lt;/p&gt;&#10;&#10;&lt;p&gt;Return values&lt;/p&gt;&#10;&#10;&lt;p&gt;iso_s16&lt;/p&gt;&#10;&#10;&lt;ul&gt;&#10;&#9;&lt;li&gt;&lt;span style=&quot;color:#0066cc&quot;&gt;E_NO_ERR&lt;/span&gt; - OK&lt;/li&gt;&#10;&#9;&lt;li&gt;&lt;span style=&quot;color:#0066cc&quot;&gt;E_OVERFLOW&lt;/span&gt; - buffer overflow&lt;/li&gt;&#10;&#9;&lt;li&gt;&lt;span style=&quot;color:#0066cc&quot;&gt;E_NOACT&lt;/span&gt; - Command not possible in current state&lt;/li&gt;&#10;&#9;&lt;li&gt;&lt;span style=&quot;color:#0066cc&quot;&gt;E_NO_INSTANCE&lt;/span&gt; - No VT client available&lt;/li&gt;&#10;&lt;/ul&gt;&#10;" >
	</Identification>
	<VersionInfo Version="1.0" Author="franz" Date="2022-07-08">
	</VersionInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="INIT" Type="Event" Comment="Service Initialization">
				<With Var="u16ObjId"/>
			</Event>
			<Event Name="REQ" Type="Event" Comment="Service Request">
				<With Var="u32NewValue"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="INITO" Type="Event" Comment="Initialization Confirm">
			</Event>
			<Event Name="CNF" Type="Event" Comment="Confirmation of Requested Service">
				<With Var="STATUS"/>
				<With Var="u32OldValue"/>
				<With Var="result"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="u16ObjId" Type="UINT" Comment="Object ID"/>
			<VarDeclaration Name="u32NewValue" Type="UDINT" Comment="New value of ID"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="STATUS" Type="WSTRING" Comment="Service Status"/>
			<VarDeclaration Name="u32OldValue" Type="UDINT" Comment="Old value of ID"/>
			<VarDeclaration Name="result" Type="INT" Comment="retval see description"/>
		</OutputVars>
	</InterfaceList>
</FBType>
