<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="Q_ObjHideShow" Comment="Command Hide/Show Object (Container) ( Part 6 - F.2 )">
	<Identification Standard="ISO 11783-6" Description="&lt;p&gt;This command is used to change the value of an object. It applies only to objects that have a numeric value attribute.&lt;/p&gt;&#10;&#10;&lt;p&gt;Return values&lt;/p&gt;&#10;&#10;&lt;p&gt;iso_s16&lt;/p&gt;&#10;&#10;&lt;ul&gt;&#10;&#9;&lt;li&gt;E_NO_ERR - OK&lt;/li&gt;&#10;&#9;&lt;li&gt;E_OVERFLOW - buffer overflow&lt;/li&gt;&#10;&#9;&lt;li&gt;E_NOACT - Command not possible in current state&lt;/li&gt;&#10;&#9;&lt;li&gt;E_NO_INSTANCE - No VT client available&lt;/li&gt;&#10;&lt;/ul&gt;&#10;" >
	</Identification>
	<VersionInfo Version="1.0" Author="franz" Date="2022-07-08">
	</VersionInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="INIT" Type="Event" Comment="Service Initialization">
				<With Var="u16ObjId"/>
			</Event>
			<Event Name="REQ" Type="Event" Comment="Service Request">
				<With Var="qVisible"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="INITO" Type="Event" Comment="Initialization Confirm">
			</Event>
			<Event Name="CNF" Type="Event" Comment="Confirmation of Requested Service">
				<With Var="STATUS"/>
				<With Var="qOldVisible"/>
				<With Var="result"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="u16ObjId" Type="UINT" Comment="Object ID"/>
			<VarDeclaration Name="qVisible" Type="BOOL" Comment="0 == disable, 1 == enable"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="STATUS" Type="WSTRING" Comment="Service Status"/>
			<VarDeclaration Name="qOldVisible" Type="BOOL" Comment="Old value of Visible"/>
			<VarDeclaration Name="result" Type="INT" Comment="retval see description"/>
		</OutputVars>
	</InterfaceList>
</FBType>
