<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="PKP_2200_LI_4_DI" Comment="Service Interface Function Block Type">
	<Identification Standard="SAE J1939" Function="Combo Block: 8 Inputs/8 Outputs (includes all input/output types)" Type="DP-34044-1-000" Description="this maps to IX/QX" >
	</Identification>
	<VersionInfo Version="1.0" Author="Jose Cabral" Date="2018-12-05">
	</VersionInfo>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.1" Author="Franz Höpfinger" Date="2022-08-04">
	</VersionInfo>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.2" Author="Franz Höpfinger" Date="2023-01-05">
	</VersionInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="INIT" Type="Event" Comment="Service Initialization">
				<With Var="QI"/>
				<With Var="u8SAMember"/>
				<With Var="u16ObjId"/>
				<With Var="DigitalInput_Key_1"/>
				<With Var="DigitalInput_Key_2"/>
				<With Var="DigitalInput_Key_3"/>
				<With Var="DigitalInput_Key_4"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="INITO" Type="Event" Comment="Initialization Confirm">
				<With Var="QO"/>
			</Event>
			<Event Name="IND" Type="Event" Comment="Indication from Resource">
				<With Var="QO"/>
				<With Var="STATUS"/>
				<With Var="qTimedOut"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="QI" Type="BOOL" Comment="Event Input Qualifier"/>
			<VarDeclaration Name="u8SAMember" Type="USINT" Comment="Node SA Default 21h" InitialValue="224"/>
			<VarDeclaration Name="u16ObjId" Type="UINT" Comment="Dummy ! set to 65535 if in Doubt. 0 deactivates FB." InitialValue="65535"/>
			<VarDeclaration Name="DigitalInput_Key_1" Type="WSTRING" Comment="" InitialValue="&quot;DigitalInput_Key_1&quot;"/>
			<VarDeclaration Name="DigitalInput_Key_2" Type="WSTRING" Comment="" InitialValue="&quot;DigitalInput_Key_2&quot;"/>
			<VarDeclaration Name="DigitalInput_Key_3" Type="WSTRING" Comment="" InitialValue="&quot;DigitalInput_Key_3&quot;"/>
			<VarDeclaration Name="DigitalInput_Key_4" Type="WSTRING" Comment="" InitialValue="&quot;DigitalInput_Key_4&quot;"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="QO" Type="BOOL" Comment="Event Output Qualifier"/>
			<VarDeclaration Name="STATUS" Type="WSTRING" Comment="Service Status"/>
			<VarDeclaration Name="qTimedOut" Type="BOOL" Comment=""/>
		</OutputVars>
	</InterfaceList>
</FBType>
