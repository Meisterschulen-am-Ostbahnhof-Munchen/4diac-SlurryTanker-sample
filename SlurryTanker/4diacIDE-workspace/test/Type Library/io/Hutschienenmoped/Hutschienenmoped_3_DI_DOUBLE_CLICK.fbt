<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="Hutschienenmoped_3_DI_DOUBLE_CLICK" Comment="Service Interface Function Block Type">
	<Identification Function="All Inputs"  Description="this maps to IE" >
	</Identification>
	<VersionInfo Version="1.0" Author="Jose Cabral" Date="2018-12-05">
	</VersionInfo>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.1" Author="Franz Höpfinger" Date="2022-08-04">
	</VersionInfo>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.2" Author="Franz Höpfinger" Date="2023-01-05">
	</VersionInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="INIT" Type="Event" Comment="Service Initialization">
				<With Var="QI"/>
				<With Var="u8SAMember"/>
				<With Var="u16ObjId"/>
				<With Var="DigitalInput_I1"/>
				<With Var="DigitalInput_I2"/>
				<With Var="DigitalInput_I3"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="INITO" Type="Event" Comment="Initialization Confirm">
				<With Var="QO"/>
			</Event>
			<Event Name="IND" Type="Event" Comment="Indication from Resource">
				<With Var="QO"/>
				<With Var="STATUS"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="QI" Type="BOOL" Comment="Event Input Qualifier"/>
			<VarDeclaration Name="u8SAMember" Type="USINT" Comment="Dummy !, set to 255 if in doubt. 0 deactivates FB." InitialValue="255"/>
			<VarDeclaration Name="u16ObjId" Type="UINT" Comment="Dummy ! set to 65535 if in Doubt. 0 deactivates FB." InitialValue="65535"/>
			<VarDeclaration Name="DigitalInput_I1" Type="WSTRING" Comment="Identifier for Input 1" InitialValue="&quot;DigitalInput_I1_DOUBLE_CLICK&quot;"/>
			<VarDeclaration Name="DigitalInput_I2" Type="WSTRING" Comment="Identifier for Input 2" InitialValue="&quot;DigitalInput_I2_DOUBLE_CLICK&quot;"/>
			<VarDeclaration Name="DigitalInput_I3" Type="WSTRING" Comment="Identifier for Input 3" InitialValue="&quot;DigitalInput_I3_DOUBLE_CLICK&quot;"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="QO" Type="BOOL" Comment="Event Output Qualifier"/>
			<VarDeclaration Name="STATUS" Type="WSTRING" Comment="Service Status"/>
		</OutputVars>
	</InterfaceList>
</FBType>
