<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="UT_SoftkeyMapping" Comment="Service Interface Function Block Type">
	<Identification Standard=" ISO 11783-6" Classification="Soft Key – ISO 11783-6 – B.6" Function="All Inputs" Type="UT according to ISO 11783-6"  Description="this maps to IX" >
	</Identification>
	<VersionInfo Version="1.0" Author="Jose Cabral" Date="2018-12-05">
	</VersionInfo>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.1" Author="Franz Höpfinger" Date="2022-08-04">
	</VersionInfo>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.2" Author="Franz Höpfinger" Date="2023-01-05">
	</VersionInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="INIT" Type="Event" Comment="Service Initialization">
				<With Var="QI"/>
				<With Var="u8SAMember"/>
				<With Var="u16ObjId_1"/>
				<With Var="DigitalInput_1"/>
				<With Var="u16ObjId_2"/>
				<With Var="DigitalInput_2"/>
				<With Var="u16ObjId_3"/>
				<With Var="DigitalInput_3"/>
				<With Var="u16ObjId_4"/>
				<With Var="DigitalInput_4"/>
				<With Var="u16ObjId_5"/>
				<With Var="DigitalInput_5"/>
				<With Var="u16ObjId_6"/>
				<With Var="DigitalInput_6"/>
				<With Var="u16ObjId_7"/>
				<With Var="DigitalInput_7"/>
				<With Var="u16ObjId_8"/>
				<With Var="DigitalInput_8"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="INITO" Type="Event" Comment="Initialization Confirm">
				<With Var="QO"/>
			</Event>
			<Event Name="IND" Type="Event" Comment="Indication from Resource">
				<With Var="QO"/>
				<With Var="STATUS"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="QI" Type="BOOL" Comment="Event Input Qualifier"/>
			<VarDeclaration Name="u8SAMember" Type="USINT" Comment="Dummy !, set to 255 if in doubt. 0 deactivates FB." InitialValue="255"/>
			<VarDeclaration Name="u16ObjId_1" Type="UINT" Comment="Object ID of Input 1" InitialValue="5000"/>
			<VarDeclaration Name="DigitalInput_1" Type="WSTRING" Comment="Identifier for Input 1" InitialValue="&quot;SoftKey_5000&quot;"/>
			<VarDeclaration Name="u16ObjId_2" Type="UINT" Comment="Object ID of Input 2" InitialValue="5001"/>
			<VarDeclaration Name="DigitalInput_2" Type="WSTRING" Comment="Identifier for Input 2" InitialValue="&quot;SoftKey_5001&quot;"/>
			<VarDeclaration Name="u16ObjId_3" Type="UINT" Comment="Object ID of Input 3" InitialValue="5002"/>
			<VarDeclaration Name="DigitalInput_3" Type="WSTRING" Comment="Identifier for Input 3" InitialValue="&quot;SoftKey_5002&quot;"/>
			<VarDeclaration Name="u16ObjId_4" Type="UINT" Comment="Object ID of Input 4" InitialValue="5003"/>
			<VarDeclaration Name="DigitalInput_4" Type="WSTRING" Comment="Identifier for Input 4" InitialValue="&quot;SoftKey_5003&quot;"/>
			<VarDeclaration Name="u16ObjId_5" Type="UINT" Comment="Object ID of Input 5" InitialValue="5004"/>
			<VarDeclaration Name="DigitalInput_5" Type="WSTRING" Comment="Identifier for Input 5" InitialValue="&quot;SoftKey_5004&quot;"/>
			<VarDeclaration Name="u16ObjId_6" Type="UINT" Comment="Object ID of Input 6" InitialValue="5005"/>
			<VarDeclaration Name="DigitalInput_6" Type="WSTRING" Comment="Identifier for Input 6" InitialValue="&quot;SoftKey_5005&quot;"/>
			<VarDeclaration Name="u16ObjId_7" Type="UINT" Comment="Object ID of Input 7" InitialValue="5006"/>
			<VarDeclaration Name="DigitalInput_7" Type="WSTRING" Comment="Identifier for Input 7" InitialValue="&quot;SoftKey_5006&quot;"/>
			<VarDeclaration Name="u16ObjId_8" Type="UINT" Comment="Object ID of Input 8" InitialValue="5007"/>
			<VarDeclaration Name="DigitalInput_8" Type="WSTRING" Comment="Identifier for Input 8" InitialValue="&quot;SoftKey_5007&quot;"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="QO" Type="BOOL" Comment="Event Output Qualifier"/>
			<VarDeclaration Name="STATUS" Type="WSTRING" Comment="Service Status"/>
		</OutputVars>
	</InterfaceList>
</FBType>
