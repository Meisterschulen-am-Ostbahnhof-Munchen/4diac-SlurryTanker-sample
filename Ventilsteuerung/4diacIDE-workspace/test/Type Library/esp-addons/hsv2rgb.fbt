	</Identification>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.0" Author="Franz HÃ¶pfinger" Date="2022-07-16">
	</VersionInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="REQ" Type="Event" Comment="Service Request" >
				<With Var="hue"/>
				<With Var="saturation"/>
				<With Var="value"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="CNF" Type="Event" Comment="Confirmation of Requested Service" >
				<With Var="r"/>
				<With Var="g"/>
				<With Var="b"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="hue" Type="UDINT" Comment="hue" />
			<VarDeclaration Name="saturation" Type="UDINT" Comment="saturation" />
			<VarDeclaration Name="value" Type="UDINT" Comment="value" />
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="r" Type="UDINT" Comment="red" />
			<VarDeclaration Name="g" Type="UDINT" Comment="green" />
			<VarDeclaration Name="b" Type="UDINT" Comment="blue" />
		</OutputVars>
	</InterfaceList>
</FBType>
