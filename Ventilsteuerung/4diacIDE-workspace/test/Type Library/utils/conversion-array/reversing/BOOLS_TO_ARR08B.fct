	<FunctionBody>
		<ST><![CDATA[FUNCTION BOOLS_TO_ARR08B

VAR_INPUT
	IN_00 : BOOL;    // BIT 00
	IN_01 : BOOL;    // BIT 01
	IN_02 : BOOL;    // BIT 02
	IN_03 : BOOL;    // BIT 03
	IN_04 : BOOL;    // BIT 04
	IN_05 : BOOL;    // BIT 05
	IN_06 : BOOL;    // BIT 06
	IN_07 : BOOL;    // BIT 07
END_VAR

VAR_OUTPUT
	OUT : ARRAY[0..7] OF BOOL;    // ARR08B Output
END_VAR

(* this Function extracts the 8 BOOLS from a byte *)
OUT[0] := IN_00;
OUT[1] := IN_01;
OUT[2] := IN_02;
OUT[3] := IN_03;
OUT[4] := IN_04;
OUT[5] := IN_05;
OUT[6] := IN_06;
OUT[7] := IN_07;

END_FUNCTION
