	</Identification>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.0" Author="Franz HÃ¶pfinger" Date="2024-02-22">
	</VersionInfo>
	<CompilerInfo>
	</CompilerInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="REQ" Type="Event" Comment="Normal Execution Request" >
				<With Var="IN"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="CNF" Type="Event" Comment="Execution Confirmation" >
				<With Var="OUT"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="IN" Type="BOOL" Comment="ARR08B Input"  ArraySize="0..7"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="OUT" Type="ST08B" Comment="ST08B Output" />
		</OutputVars>
	</InterfaceList>
	<FunctionBody>
		<ST><![CDATA[FUNCTION ARR08B_TO_ST08B

VAR_INPUT
	IN : ARRAY[0..7] OF BOOL;    // ARR08B Input
END_VAR

VAR_OUTPUT
	OUT : ST08B;    // ST08B Output
END_VAR

(* this Function extracts the 8 BOOLS from a byte *)
OUT.B_00 := IN[0];
OUT.B_01 := IN[1];
OUT.B_02 := IN[2];
OUT.B_03 := IN[3];
OUT.B_04 := IN[4];
OUT.B_05 := IN[5];
OUT.B_06 := IN[6];
OUT.B_07 := IN[7];

END_FUNCTION
]]></ST>
	</FunctionBody>
</Function>
