	<FunctionBody>
		<ST><![CDATA[FUNCTION ARR08B_TO_BYTE

VAR_INPUT
	IN : ARRAY[0..7] OF BOOL;    // ARR08B Input
END_VAR

VAR_OUTPUT
	OUT : BYTE;    // BYTE Output
END_VAR

(* this Function extracts the 8 Bits from a byte *)
OUT.0 := IN[0];
OUT.1 := IN[1];
OUT.2 := IN[2];
OUT.3 := IN[3];
OUT.4 := IN[4];
OUT.5 := IN[5];
OUT.6 := IN[6];
OUT.7 := IN[7];

END_FUNCTION
]]></ST>
	</FunctionBody>
</Function>
