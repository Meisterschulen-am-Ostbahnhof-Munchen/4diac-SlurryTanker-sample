<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="sequence_ET_05" Comment="Sequence with 5 Outputs, Transition by EVENT or TIME">
	<Identification Standard="61499-2">
	</Identification>
	<VersionInfo Version="1.0" Author="franz" Date="2024-03-02">
	</VersionInfo>
	<CompilerInfo>
	</CompilerInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="START_S1" Type="Event" Comment="jump from START to State_01">
				<With Var="DT_S1_S2"/>
				<With Var="DT_S2_S3"/>
				<With Var="DT_S3_S4"/>
				<With Var="DT_S4_S5"/>
				<With Var="DT_S5_START"/>
			</Event>
			<Event Name="S1_S2" Type="Event" Comment="jump from State_01 to State_02">
			</Event>
			<Event Name="S2_S3" Type="Event" Comment="jump from State_02 to State_03">
			</Event>
			<Event Name="S3_S4" Type="Event" Comment="jump from State_03 to State_04">
			</Event>
			<Event Name="S4_S5" Type="Event" Comment="jump from State_04 to State_05">
			</Event>
			<Event Name="S5_START" Type="Event" Comment="jump from State_05 to START">
			</Event>
			<Event Name="RESET" Type="Event" Comment="Reset from any State to START">
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="CNF" Type="Event" Comment="Execution Confirmation">
				<With Var="STATE_NR"/>
			</Event>
			<Event Name="EO_S1" Type="Event" Comment="">
				<With Var="DO_S1"/>
			</Event>
			<Event Name="EO_S2" Type="Event" Comment="">
				<With Var="DO_S2"/>
			</Event>
			<Event Name="EO_S3" Type="Event" Comment="">
				<With Var="DO_S3"/>
			</Event>
			<Event Name="EO_S4" Type="Event" Comment="">
				<With Var="DO_S4"/>
			</Event>
			<Event Name="EO_S5" Type="Event" Comment="">
				<With Var="DO_S5"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="DT_S1_S2" Type="TIME" Comment="State_01 to State_02 timed, set to NO_TIME to disable" InitialValue="NO_TIME"/>
			<VarDeclaration Name="DT_S2_S3" Type="TIME" Comment="State_02 to State_03 timed, set to NO_TIME to disable" InitialValue="NO_TIME"/>
			<VarDeclaration Name="DT_S3_S4" Type="TIME" Comment="State_03 to State_04 timed, set to NO_TIME to disable" InitialValue="NO_TIME"/>
			<VarDeclaration Name="DT_S4_S5" Type="TIME" Comment="State_04 to State_05 timed, set to NO_TIME to disable" InitialValue="NO_TIME"/>
			<VarDeclaration Name="DT_S5_START" Type="TIME" Comment="State_05 to START timed, set to NO_TIME to disable" InitialValue="NO_TIME"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="STATE_NR" Type="SINT" Comment="State Number, START = 0, State_01 = 1 and so on. "/>
			<VarDeclaration Name="DO_S1" Type="BOOL" Comment="Output if State_01 is active"/>
			<VarDeclaration Name="DO_S2" Type="BOOL" Comment="Output if State_02 is active"/>
			<VarDeclaration Name="DO_S3" Type="BOOL" Comment="Output if State_03 is active"/>
			<VarDeclaration Name="DO_S4" Type="BOOL" Comment="Output if State_04 is active"/>
			<VarDeclaration Name="DO_S5" Type="BOOL" Comment="Output if State_05 is active"/>
		</OutputVars>
		<Plugs>
			<AdapterDeclaration Name="timeOut" Type="ATimeOut" Comment="timeOut Adapter"/>
		</Plugs>
	</InterfaceList>
	<BasicFB>
		<ECC>
			<ECState Name="xSTART" Comment="Initial State" x="800" y="800">
			</ECState>
			<ECState Name="sState_01" Comment="" x="2400" y="1600">
				<ECAction Output="timeOut.STOP"/>
				<ECAction/>
				<ECAction Algorithm="State_01_C" Output="CNF"/>
				<ECAction Algorithm="State_01_E" Output="EO_S1"/>
				<ECAction Output="timeOut.START"/>
			</ECState>
			<ECState Name="sState_02" Comment="" x="2400" y="2400">
				<ECAction Output="timeOut.STOP"/>
				<ECAction Algorithm="State_01_X" Output="EO_S1"/>
				<ECAction Algorithm="State_02_C" Output="CNF"/>
				<ECAction Algorithm="State_02_E" Output="EO_S2"/>
				<ECAction Output="timeOut.START"/>
			</ECState>
			<ECState Name="sState_03" Comment="" x="2400" y="3200">
				<ECAction Output="timeOut.STOP"/>
				<ECAction Algorithm="State_02_X" Output="EO_S2"/>
				<ECAction Algorithm="State_03_C" Output="CNF"/>
				<ECAction Algorithm="State_03_E" Output="EO_S3"/>
				<ECAction Output="timeOut.START"/>
			</ECState>
			<ECState Name="sState_04" Comment="" x="2400" y="4000">
				<ECAction Output="timeOut.STOP"/>
				<ECAction Algorithm="State_03_X" Output="EO_S3"/>
				<ECAction Algorithm="State_04_C" Output="CNF"/>
				<ECAction Algorithm="State_04_E" Output="EO_S4"/>
				<ECAction Output="timeOut.START"/>
			</ECState>
			<ECState Name="sState_00" Comment="" x="2400" y="5600">
				<ECAction Output="timeOut.STOP"/>
				<ECAction Algorithm="State_05_X" Output="EO_S5"/>
				<ECAction Algorithm="State_00_C" Output="CNF"/>
				<ECAction/>
				<ECAction/>
			</ECState>
			<ECState Name="sRESET" Comment="" x="-800" y="4800">
				<ECAction Output="timeOut.STOP"/>
				<ECAction Algorithm="State_01_X" Output="EO_S1"/>
				<ECAction Algorithm="State_02_X" Output="EO_S2"/>
				<ECAction Algorithm="State_03_X" Output="EO_S3"/>
				<ECAction Algorithm="State_04_X" Output="EO_S4"/>
				<ECAction Algorithm="State_05_X" Output="EO_S5"/>
			</ECState>
			<ECState Name="sState_05" Comment="" x="2400" y="4800">
				<ECAction Output="timeOut.STOP"/>
				<ECAction Algorithm="State_04_X" Output="EO_S4"/>
				<ECAction Algorithm="State_05_C" Output="CNF"/>
				<ECAction Algorithm="State_05_E" Output="EO_S5"/>
				<ECAction Output="timeOut.START"/>
			</ECState>
			<ECTransition Source="sState_01" Destination="sState_02" Condition="S1_S2" Comment="" x="2340" y="2173.33"/>
			<ECTransition Source="sState_02" Destination="sState_03" Condition="S2_S3" Comment="" x="2426.67" y="3000"/>
			<ECTransition Source="sState_03" Destination="sState_04" Condition="S3_S4" Comment="" x="2320" y="3573.33"/>
			<ECTransition Source="sState_01" Destination="sState_02" Condition="timeOut.TimeOut" Comment="" x="2586.67" y="2046.67"/>
			<ECTransition Source="sState_02" Destination="sState_03" Condition="timeOut.TimeOut" Comment="" x="2653.33" y="2866.67"/>
			<ECTransition Source="sState_03" Destination="sState_04" Condition="timeOut.TimeOut" Comment="" x="2480" y="3700"/>
			<ECTransition Source="sState_00" Destination="sState_01" Condition="START_S1" Comment="" x="1213.33" y="2600"/>
			<ECTransition Source="xSTART" Destination="sState_01" Condition="START_S1" Comment="" x="2093.33" y="1026.67"/>
			<ECTransition Source="sState_01" Destination="sRESET" Condition="RESET" Comment="" x="613.33" y="1813.33"/>
			<ECTransition Source="sState_02" Destination="sRESET" Condition="RESET" Comment="" x="633.33" y="2593.33"/>
			<ECTransition Source="sState_03" Destination="sRESET" Condition="RESET" Comment="" x="806.67" y="3240"/>
			<ECTransition Source="sRESET" Destination="sState_00" Condition="RESET" Comment="" x="780" y="4600"/>
			<ECTransition Source="sState_04" Destination="sState_05" Condition="S4_S5" Comment="" x="2360" y="4466.67"/>
			<ECTransition Source="sState_04" Destination="sState_05" Condition="timeOut.TimeOut" Comment="" x="2600" y="4286.67"/>
			<ECTransition Source="sState_04" Destination="sRESET" Condition="RESET" Comment="" x="826.67" y="3806.67"/>
			<ECTransition Source="sState_05" Destination="sState_00" Condition="S5_START" Comment="" x="2140" y="5126.67"/>
			<ECTransition Source="sState_05" Destination="sState_00" Condition="timeOut.TimeOut" Comment="" x="2606.67" y="5206.67"/>
			<ECTransition Source="sState_05" Destination="sRESET" Condition="RESET" Comment="" x="800" y="4340"/>
		</ECC>
		<Algorithm Name="State_00_C" Comment="">
			<ST><![CDATA[    // Confirmation STEPS
ALGORITHM State_00_C    // State_00 Confirmation Step
STATE_NR := State_00;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_01_C" Comment="">
			<ST><![CDATA[

ALGORITHM State_01_C    // State_01 Confirmation Step
STATE_NR := State_01;
timeOut.DT := DT_S1_S2;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_02_C" Comment="">
			<ST><![CDATA[

ALGORITHM State_02_C    // State_02 Confirmation Step
STATE_NR := State_02;
timeOut.DT := DT_S2_S3;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_03_C" Comment="">
			<ST><![CDATA[

ALGORITHM State_03_C    // State_03 Confirmation Step
STATE_NR := State_03;
timeOut.DT := DT_S3_S4;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_04_C" Comment="">
			<ST><![CDATA[

ALGORITHM State_04_C    // State_04 Confirmation Step
STATE_NR := State_04;
timeOut.DT := DT_S4_S5;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_05_C" Comment="">
			<ST><![CDATA[

ALGORITHM State_05_C    // State_05 Confirmation Step
STATE_NR := State_05;
timeOut.DT := DT_S5_START;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_01_E" Comment="">
			<ST><![CDATA[

ALGORITHM State_01_E    // State_01 Entry Step
DO_S1 := BOOL#TRUE;    // ON
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_01_X" Comment="">
			<ST><![CDATA[

ALGORITHM State_01_X    // State_01 Exit Step
DO_S1 := BOOL#FALSE;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_02_E" Comment="">
			<ST><![CDATA[

ALGORITHM State_02_E    // State_02 Entry Step
DO_S2 := BOOL#TRUE;    // ON
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_02_X" Comment="">
			<ST><![CDATA[

ALGORITHM State_02_X    // State_02 Exit Step
DO_S2 := BOOL#FALSE;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_03_E" Comment="">
			<ST><![CDATA[

ALGORITHM State_03_E    // State_03 Entry Step
DO_S3 := BOOL#TRUE;    // ON
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_03_X" Comment="">
			<ST><![CDATA[

ALGORITHM State_03_X    // State_03 Exit Step
DO_S3 := BOOL#FALSE;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_04_E" Comment="">
			<ST><![CDATA[

ALGORITHM State_04_E    // State_04 Entry Step
DO_S4 := BOOL#TRUE;    // ON
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_04_X" Comment="">
			<ST><![CDATA[

ALGORITHM State_04_X    // State_04 Exit Step
DO_S4 := BOOL#FALSE;
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_05_E" Comment="">
			<ST><![CDATA[

ALGORITHM State_05_E    // State_05 Entry Step
DO_S5 := BOOL#TRUE;    // ON
END_ALGORITHM]]></ST>
		</Algorithm>
		<Algorithm Name="State_05_X" Comment="">
			<ST><![CDATA[

ALGORITHM State_05_X    // State_05 Exit Step
DO_S5 := BOOL#FALSE;
END_ALGORITHM

]]></ST>
		</Algorithm>
	</BasicFB>
</FBType>
