<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="OFFSET_UDINT" Comment="Simple FB with one algorithm">
	<Identification Standard="61499-1" Description="Copyright (c) 2024 HR Agrartechnik GmbH  &#10; &#10;This program and the accompanying materials are made&#10;available under the terms of the Eclipse Public License 2.0&#10;which is available at https://www.eclipse.org/legal/epl-2.0/&#10;&#10;SPDX-License-Identifier: EPL-2.0" >
	</Identification>
	<VersionInfo Version="1.0" Author="franz" Date="2024-10-18">
	</VersionInfo>
	<CompilerInfo packageName="utils::math">
	</CompilerInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="REQ" Type="Event" Comment="Normal Execution Request">
				<With Var="IN"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="CNF" Type="Event" Comment="Execution Confirmation">
				<With Var="OUT"/>
				<With Var="FIRST"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="IN" Type="UDINT" Comment="Input. First call with a Value &amp;gt;0 will Set FIRST" InitialValue="0"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="OUT" Type="UDINT" Comment="IN - FIRST" InitialValue="0"/>
			<VarDeclaration Name="FIRST" Type="UDINT" Comment="Set at First Call" InitialValue="0"/>
		</OutputVars>
	</InterfaceList>
	<SimpleFB>
		<Algorithm Name="REQ" Comment="">
			<ST><![CDATA[ALGORITHM REQ

IF (FIRST = 0) THEN
	FIRST := IN;
END_IF;

OUT := IN - FIRST;

END_ALGORITHM

]]></ST>
		</Algorithm>
	</SimpleFB>
</FBType>
