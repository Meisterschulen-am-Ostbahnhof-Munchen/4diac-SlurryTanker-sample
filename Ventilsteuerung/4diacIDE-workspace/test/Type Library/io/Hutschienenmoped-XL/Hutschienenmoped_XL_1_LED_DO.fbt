<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="Hutschienenmoped_XL_1_LED_DO" Comment="Service Interface Function Block Type">
	<Identification Function="All Inputs" Description="this maps to QX &#10;&#10;uses LED_GPIO_MODE" >
	</Identification>
	<VersionInfo Version="1.0" Author="Jose Cabral" Date="2018-12-05">
	</VersionInfo>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.1" Author="Franz Hoepfinger" Date="2022-08-04">
	</VersionInfo>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.2" Author="Franz Hoepfinger" Date="2023-01-05">
	</VersionInfo>
	<CompilerInfo>
	</CompilerInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="INIT" Type="Event" Comment="Service Initialization">
				<With Var="QI"/>
				<With Var="u8SAMember"/>
				<With Var="u16ObjId"/>
				<With Var="LED_5HZ"/>
				<With Var="LED_2HZ"/>
				<With Var="LED_1HZ"/>
				<With Var="LED_05HZ"/>
				<With Var="LED_ON"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="INITO" Type="Event" Comment="Initialization Confirm">
				<With Var="QO"/>
			</Event>
			<Event Name="IND" Type="Event" Comment="Indication from Resource">
				<With Var="QO"/>
				<With Var="STATUS"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="QI" Type="BOOL" Comment="Event Input Qualifier"/>
			<VarDeclaration Name="u8SAMember" Type="USINT" Comment="Identify the Output Output_Q1..Q8" InitialValue="Output_Q1"/>
			<VarDeclaration Name="u16ObjId" Type="UINT" Comment="Dummy ! set to ID_NULL if in Doubt. ID_OFF deactivates FB." InitialValue="ID_NULL"/>
			<VarDeclaration Name="LED_5HZ" Type="STRING" Comment="Identifier for LED blinking 5HZ, highest Prio (Prio 6)"/>
			<VarDeclaration Name="LED_2HZ" Type="STRING" Comment="Identifier for LED blinking 2HZ, Prio 5"/>
			<VarDeclaration Name="LED_1HZ" Type="STRING" Comment="Identifier for LED blinking 1HZ, Prio 4"/>
			<VarDeclaration Name="LED_05HZ" Type="STRING" Comment="Identifier for LED blinking 0.5Hz, Prio 3"/>
			<VarDeclaration Name="LED_ON" Type="STRING" Comment="Identifier for LED always on, Prio 1"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="QO" Type="BOOL" Comment="Event Output Qualifier"/>
			<VarDeclaration Name="STATUS" Type="STRING" Comment="Service Status"/>
		</OutputVars>
	</InterfaceList>
</FBType>
