<?xml version="1.0" encoding="UTF-8"?>
<FBType Name="TC_InputMapping" Comment="Service Interface Function Block Type">
	<Identification Standard="ISO 11783-10" Function="All Inputs" Type="TC according to ISO 11783-10" Description="this maps to ID &#10;32 bit Process Data (PD)" >
	</Identification>
	<VersionInfo Version="1.0" Author="Jose Cabral" Date="2018-12-05">
	</VersionInfo>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.1" Author="Franz Hoepfinger" Date="2022-08-04">
	</VersionInfo>
	<VersionInfo Organization="HR Agrartechnik GmbH" Version="1.2" Author="Franz Hoepfinger" Date="2023-01-05">
	</VersionInfo>
	<InterfaceList>
		<EventInputs>
			<Event Name="INIT" Type="Event" Comment="Service Initialization">
				<With Var="QI"/>
				<With Var="u8SAMember"/>
				<With Var="u16ObjId"/>
				<With Var="wDevElementNumb_1"/>
				<With Var="wDDI_1"/>
				<With Var="Input_1"/>
				<With Var="wDevElementNumb_2"/>
				<With Var="wDDI_2"/>
				<With Var="Input_2"/>
				<With Var="wDevElementNumb_3"/>
				<With Var="wDDI_3"/>
				<With Var="Input_3"/>
				<With Var="wDevElementNumb_4"/>
				<With Var="wDDI_4"/>
				<With Var="Input_4"/>
				<With Var="wDevElementNumb_5"/>
				<With Var="wDDI_5"/>
				<With Var="Input_5"/>
				<With Var="wDevElementNumb_6"/>
				<With Var="wDDI_6"/>
				<With Var="Input_6"/>
				<With Var="wDevElementNumb_7"/>
				<With Var="wDDI_7"/>
				<With Var="Input_7"/>
				<With Var="wDevElementNumb_8"/>
				<With Var="wDDI_8"/>
				<With Var="Input_8"/>
			</Event>
		</EventInputs>
		<EventOutputs>
			<Event Name="INITO" Type="Event" Comment="Initialization Confirm">
				<With Var="QO"/>
			</Event>
			<Event Name="IND" Type="Event" Comment="Indication from Resource">
				<With Var="QO"/>
				<With Var="STATUS"/>
			</Event>
		</EventOutputs>
		<InputVars>
			<VarDeclaration Name="QI" Type="BOOL" Comment="Event Input Qualifier"/>
			<VarDeclaration Name="u8SAMember" Type="USINT" Comment="Dummy !, set to GLOBAL if in doubt. OFF deactivates FB." InitialValue="GLOBAL"/>
			<VarDeclaration Name="u16ObjId" Type="UINT" Comment="Dummy ! set to ID_NULL if in Doubt. ID_OFF deactivates FB." InitialValue="ID_NULL"/>
			<VarDeclaration Name="wDevElementNumb_1" Type="UINT" Comment="Requested element number of TC of Input 1"/>
			<VarDeclaration Name="wDDI_1" Type="UINT" Comment="Data dictionary identifier of Input 1"/>
			<VarDeclaration Name="Input_1" Type="STRING" Comment="Identifier for Input 1"/>
			<VarDeclaration Name="wDevElementNumb_2" Type="UINT" Comment="Requested element number of TC of Input 2"/>
			<VarDeclaration Name="wDDI_2" Type="UINT" Comment="Data dictionary identifier of Input 2"/>
			<VarDeclaration Name="Input_2" Type="STRING" Comment="Identifier for Input 2"/>
			<VarDeclaration Name="wDevElementNumb_3" Type="UINT" Comment="Requested element number of TC of Input 3"/>
			<VarDeclaration Name="wDDI_3" Type="UINT" Comment="Data dictionary identifier of Input 3"/>
			<VarDeclaration Name="Input_3" Type="STRING" Comment="Identifier for Input 3"/>
			<VarDeclaration Name="wDevElementNumb_4" Type="UINT" Comment="Requested element number of TC of Input 4"/>
			<VarDeclaration Name="wDDI_4" Type="UINT" Comment="Data dictionary identifier of Input 4"/>
			<VarDeclaration Name="Input_4" Type="STRING" Comment="Identifier for Input 4"/>
			<VarDeclaration Name="wDevElementNumb_5" Type="UINT" Comment="Requested element number of TC of Input 5"/>
			<VarDeclaration Name="wDDI_5" Type="UINT" Comment="Data dictionary identifier of Input 5"/>
			<VarDeclaration Name="Input_5" Type="STRING" Comment="Identifier for Input 5"/>
			<VarDeclaration Name="wDevElementNumb_6" Type="UINT" Comment="Requested element number of TC of Input 6"/>
			<VarDeclaration Name="wDDI_6" Type="UINT" Comment="Data dictionary identifier of Input 6"/>
			<VarDeclaration Name="Input_6" Type="STRING" Comment="Identifier for Input 6"/>
			<VarDeclaration Name="wDevElementNumb_7" Type="UINT" Comment="Requested element number of TC of Input 7"/>
			<VarDeclaration Name="wDDI_7" Type="UINT" Comment="Data dictionary identifier of Input 7"/>
			<VarDeclaration Name="Input_7" Type="STRING" Comment="Identifier for Input 7"/>
			<VarDeclaration Name="wDevElementNumb_8" Type="UINT" Comment="Requested element number of TC of Input 8"/>
			<VarDeclaration Name="wDDI_8" Type="UINT" Comment="Data dictionary identifier of Input 8"/>
			<VarDeclaration Name="Input_8" Type="STRING" Comment="Identifier for Input 8"/>
		</InputVars>
		<OutputVars>
			<VarDeclaration Name="QO" Type="BOOL" Comment="Event Output Qualifier"/>
			<VarDeclaration Name="STATUS" Type="STRING" Comment="Service Status"/>
		</OutputVars>
	</InterfaceList>
</FBType>
